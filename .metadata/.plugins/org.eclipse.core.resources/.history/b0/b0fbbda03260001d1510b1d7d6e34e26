package com.learn.opp.collection;

import java.util.ArrayList;
import java.util.Iterator;

public class EmployeeArrayList {

	public static void main(String[] args) {
		Employee empl=new Employee(1,"ajay",10000);
		Employee empl1=new Employee(2,"harish",20000);
		Employee empl2=new Employee(3,"shiva",30000);
		ArrayList<Employee> aemp=new ArrayList<>();
		aemp.add(empl);
		aemp.add(empl1);
		aemp.add(empl2);
		Iterator<Employee> itr= aemp.iterator();
	    while(itr.hasNext())
	    System.out.println(itr.next());
		

	}

}
