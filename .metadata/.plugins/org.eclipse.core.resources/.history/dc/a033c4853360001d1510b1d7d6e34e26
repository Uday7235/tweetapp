package com.learn.opp.collection;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.Iterator;

public class EmployeeHashMap {

	public static void main(String[] args) {
		Employee empl=new Employee(1,"ajay",10000);
		Employee empl1=new Employee(2,"harish",20000);
		Employee empl2=new Employee(3,"shiva",30000);
		HashMap<Integer,Employee> aemp=new HashMap<Integer,Employee>();
		aemp.put(1,empl);
		aemp.put(2,empl1);
		aemp.put(3,empl2);
		Iterator<String,Employee> itr= aemp.iterator();
	    while(itr.hasNext())
	    System.out.println(itr.next());

	}

}
